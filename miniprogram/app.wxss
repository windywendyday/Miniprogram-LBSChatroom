/**app.wxss**/

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

.mainButton {
  width: 300rpx;
  height: 80rpx;

  /* 主题黄 */
  background-color: #F4C243;
  border-radius: 20px;
  text-align: center;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.welcomeContainer{
  width: 100%;
  height: 300rpx;
  /*background-color: green;*/
}

.backgroundBar{
  position: absolute;
  left: 48rpx;
  top: 150rpx;
  width: 500rpx;
  height: 30rpx;
  /* 浅绿2 */
  background: #DAF2EA;
}
.welcomeText{
  position: absolute;
  left: 74rpx;
  top: 120rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 600;
  font-size: 50rpx;
  line-height: 100%;
  /* identical to box height, or 34px */
  text-align: center;
  letter-spacing: 0.05em;

  /* 深绿1 */
  color: #1A4033;
}
